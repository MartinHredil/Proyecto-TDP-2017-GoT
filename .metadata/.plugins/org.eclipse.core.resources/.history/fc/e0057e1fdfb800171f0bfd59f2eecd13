package herramientas;

import personajes.*;

public class FabricaPersonal implements FabricaPersonajes{

	public FabricaPersonal()
	{
		
	}
	
	public Personaje crearSoldado()
	{
		return new Soldado(null);
	}
	public Personaje crearArquero()
	{
		return new Arquero(null);
	}
	public Personaje crearDragon()
	{
		return new Dragon(null);
	}
}
