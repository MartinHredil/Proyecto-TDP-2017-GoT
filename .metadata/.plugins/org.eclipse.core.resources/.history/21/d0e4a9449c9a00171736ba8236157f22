package enemigos;
import java.util.Iterator;

import juego.*;

public class MoverEnemigos extends Thread {

	protected Juego juego;
	
	public MoverEnemigos(Juego j)
	{
		juego=j;
	}
	
	public void run()
	{
		Iterator<Enemigos> it;
		for(int i=0;i<3;i++)
		{
			it = juego.getEnemigos();
			while(it.hasNext())
			{
				it.next().mover();
			}
			
		}
	}
}
