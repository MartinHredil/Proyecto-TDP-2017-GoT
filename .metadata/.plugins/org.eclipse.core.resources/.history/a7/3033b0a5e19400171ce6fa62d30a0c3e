package mapa;

public abstract class Contenido {
	protected Celda miCelda;
	protected int vida;
	
	Contenido(Celda c, int v)
	{
		vida = v;
		miCelda = c;
	}
	
	Celda getCelda()
	{
		return miCelda;
	}
	
	int getVida()
	{
		return vida;
	}
	
	void setVida(int v)
	{
		vida = v;
	}
	
	void setCelda(Celda c)
	{
		miCelda = c;
	}
}
