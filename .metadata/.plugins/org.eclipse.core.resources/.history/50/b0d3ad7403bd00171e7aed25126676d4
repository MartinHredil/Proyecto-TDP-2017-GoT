import mapa.*;

public class Soldado extends Personaje{

	
	public Soldado(Celda c)
	{
		super(c);
		vida = 1000;
		danioAtaque=5;
		grafico.setIcon(new ImageIcon(this.getClass().getResource("/sources/SoldadoEsperando.gif")));
	}
}
