package personajes;

import mapa.*;

public abstract class Personajes extends Contenido {

	protected int danioAtaque;
	
	public Personajes(Celda c, int v, int d)
	{
		super(c);
		danioAtaque = d;
	}
	
	public int getDanioAtaque()
	{
		return danioAtaque;
	}
	
	public void setDanioAtaque(int d)
	{
		danioAtaque = d;
	}
}
