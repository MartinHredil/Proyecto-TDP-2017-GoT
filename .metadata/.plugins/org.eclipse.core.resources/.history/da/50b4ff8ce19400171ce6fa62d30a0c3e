package mapa;
import herramientas.Coleccion;

public class Celda {
	
	protected int fila;
	protected int columna;
	protected Mapa mapa;
	protected Coleccion<Contenido> misContenidos;
	
	Celda(int f, int c, Mapa m)
	{
		mapa = m;
		fila = f;
		columna = c;
		misContenidos = new Coleccion<Contenido>(); 
	}
	
	int getFila()
	{
		return fila;
	}
	
	int getColumna()
	{
		return columna;
	}
	
	Iterable<Contenido> getContenidos()
	{
		return misContenidos;		
	}

}
