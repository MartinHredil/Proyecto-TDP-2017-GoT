package herramientas;

import mapa.Celda;
import objetos.*;

public class FabricaCosas implements FabricaObjetos{

	public FabricaCosas(){}
	
	public Objeto crearPiedra(Celda c)
	{
		return new Piedra(c);
	}
	public Objeto crearMuralla(Celda c)
	{
		return new Muralla(c);
	}
	public Objeto crearBomba(Celda c)
	{
		return new Bomba(c);
	}
}
